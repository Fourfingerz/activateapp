<h1 id="organizations-index">Listing organizations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Address</th>
      <th>Address 2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip Code</th>
      <th>EIN #</th>
      <th>Telephone</th>
      <th>Contact</th>
      <th>NTEE Category</th>
      <th>Keywords</th>
      <th>Categories</th>
      <th>Sub-Categories</th>
      <th>Mission</th>
      <th>Ruling Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <%= @organizations %>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.title %></td>
        <td><%= organization.url %></td>
        <td><%= organization.address %></td>
        <td><%= organization.address_2 %></td>
        <td><%= organization.city %></td>
        <td><%= organization.state %></td>
        <td><%= organization.zip %></td>
        <td><%= organization.ein %></td>
        <td><%= organization.telephone %></td>
        <td><%= organization.contact %></td>
        <td><%= organization.ntee_category %></td>
        <td><%= organization.keywords %></td>
        <td><%= organization.categories %></td>
        <td><%= organization.sub_categories %></td>
        <td><%= organization.mission %></td>
        <td><%= organization.ruling_year %></td>
        <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization', new_organization_path %>

