<h1 id="actionable">Actionable</h1>
<h2><%= link_to 'Directory', controller: 'organizations', action: 'index' %></h2>
<h3 id="studio">Monorail Dinosaur Studios @2014</h3>
<h4 id="search-box"><%= form_tag(organizations_path, :method => "get") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search Organizations" %>
  <%= submit_tag "Search", :name => nil %>
  <% end %>
</h4>
<h5 id="sign-in"><%= form_tag(@user) do %>
  <%= label_tag 'email', 'Email' %>
  <%= text_field_tag 'email'%>
  <%= label_tag 'password', 'Password' %>
  <%= password_field_tag 'password' %>
  <%= submit_tag "Sign In"%>
<% end %>
</h5>